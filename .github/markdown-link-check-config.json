{
  "ignorePatterns": [
    {
      "pattern": "^http(s)?://localhost"
    },
    {
      "pattern": "https://github.com/.+/compare/v.+\\.\\.\\.v.+"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s"
}
